<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Navigate to Platform Event Record</masterLabel>
    <description>Navigate to a Record received from Platform Event</description>
    <targets>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property name="validationId" type="String" required="true" label="Validation Id" />
            <property name="channelName" type="String" required="true" label="Platform Event API Name" />
            <property name="terminateFlow" type="Boolean" required="true" label="Terminate Flow?" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>